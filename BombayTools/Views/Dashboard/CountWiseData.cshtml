
<div id="CountWiseView">
    <div class="row" id="PrincipalTable">
        <div class="col-sm-12 col-lg-5 card">
            <h5 class="mt-3 mb-0">Principal's</h5>
            <div class="table-responsive">
                <table id="tblCountWise" class="table table-striped table-bordered compact" style="width:100%">
                    <thead>
                        <tr>
                            <th style="width:16px"></th>
                            <th>Name</th>
                            <th style="width:100px">Companies</th>
                            <th style="visibility:hidden"></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var List in ViewBag.Template)
                        {
                            <tr>
                                <td></td>
                                <td>@List.Name</td>
                                <td>@List.Count</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <div class="col-lg-5">
            <div id="principalT"></div>
        </div>
    </div>
    <div class="row " id="PrincipalWiseCompany">
        <div class="col-sm-12 col-lg-8 card">
            <h5 class="mt-3 mb-0">Principal: <label id="PrincipalName"></label></h5>
            <div class="table-responsive">
                <table id="tblCompanyList" class="table table-striped table-bordered compact" style="width:100%">
                    <thead>
                        <tr>
                            <th style="width:16px"></th>
                            <th>Company</th>
                            <th>Location</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>
